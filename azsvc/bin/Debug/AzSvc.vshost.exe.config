<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	
	<configSections>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
			<section name="AzSvc.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
		</sectionGroup>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>


	<applicationSettings>
		<AzSvc.Properties.Settings>
   <setting name="PingDelaySeconds" serializeAs="String">
    <value>1</value>
   </setting>
   <setting name="Version" serializeAs="String">
    <value>Azure Ping Service 1.0</value>
   </setting>
   <setting name="StorageAccount" serializeAs="String">
    <value>eyaleastus</value>
   </setting>
   <setting name="StorageKey" serializeAs="String">
    <value>kS5hEJyvvsVgfm5vLnnwVNqDsBnJiOZeOM+epAmDdyTItqZCk65JBj1rjmX9Mo4+0V+Kpln5u2rtRbMIXCkPMw==</value>
   </setting>
   <setting name="NickName" serializeAs="String">
    <value>EyalsTest</value>
   </setting>
   <setting name="CloudBrand" serializeAs="String">
    <value>Amazon</value>
   </setting>
   <setting name="Debug" serializeAs="String">
    <value>True</value>
   </setting>
  </AzSvc.Properties.Settings>
	</applicationSettings>

	<log4net>
		<appender name="Console" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{MM/dd/yyyy hh:mm:ss tt} %-5level - %message%newline"/>
			</layout>
		</appender>
		<appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
			<file value="AzSvc.log" />

			<appendToFile value="true"/>
			<maximumFileSize value="5MB"/>
			<maxSizeRollBackups value="10"/>
			<countDirection value="1"/>
			<rollingStyle value="Size"/>
			<encoding value="utf-8"/>
			<staticLogFileName value="true"/>
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
			<layout type="log4net.Layout.PatternLayout">
				<header value="[Begin AzSvc Log]&#xD;&#xA;"/>
				<footer value="[End AzSvc Log]&#xD;&#xA;"/>
				<conversionPattern value="%date{MM/dd/yyyy hh:mm:ss tt} %-5level - %message%newline"/>
			</layout>
		</appender>
		<root>
			<level value="DEBUG" />
			<appender-ref ref="Console" />
			<appender-ref ref="RollingFile" />
		</root>
	</log4net>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>

</configuration>